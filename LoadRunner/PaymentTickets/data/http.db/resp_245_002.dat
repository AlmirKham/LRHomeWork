<script>
	function addTooltipFontAdjustment(selector) {
		try {
			var toolTipRight = document.querySelector(selector);
			if (!toolTipRight) return;

			var currentLoadTextFontSize = window.getComputedStyle(toolTipRight).getPropertyValue('font-size');
			var sizeValue = parseFloat(currentLoadTextFontSize.split('px')[0]);
			var newSizeValue = (sizeValue + 0);

			var rightTooltipAdditionalStyle = document.createElement('style');
			rightTooltipAdditionalStyle.innerHTML = selector + ":focus::after {font-size: " + newSizeValue + "px;}";
			document.body.appendChild(rightTooltipAdditionalStyle);
		} catch (err) {}
	}

	addTooltipFontAdjustment('.metaTooltipRight');
	addTooltipFontAdjustment('.metaTooltipLeft');
	addTooltipFontAdjustment('.metaTooltipCenter');
</script>
